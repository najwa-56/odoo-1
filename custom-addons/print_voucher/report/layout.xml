<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<template id="external_layout">
        <!-- Multicompany -->
        <t t-if="not o and doc" t-set="o" t-value="doc"/>


        <t t-if="not company">
            <!-- Multicompany -->
            <t t-if="company_id">
                <t t-set="company" t-value="company_id"/>
            </t>
            <t t-elif="o and 'company_id' in o and o.company_id.sudo()">
                <t t-set="company" t-value="o.company_id.sudo()"/>
            </t>
            <t t-else="else">
                <t t-set="company" t-value="res_company"/>
            </t>
        </t>

        <t t-call="print_voucher.report_header_custom" />

        <div>
             <t t-if="company.external_report_layout_id" t-call="{{company.external_report_layout_id.sudo().key}}"><t t-out="0"/></t>
        <t t-else="else" t-call="web.external_layout_standard"><t t-out="0"/></t>

        </div>

        <t t-call="print_voucher.report_footer_custom" />
    </template>
</odoo>